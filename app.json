      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.492191243375-hafehu1hpos9kebj3e7djo7q1bt8cjbg"
        }
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.prepto.prepto",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json"
    }
  }
}
